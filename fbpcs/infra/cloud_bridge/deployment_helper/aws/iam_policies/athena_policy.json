{
    "Version": "2012-10-17",
    "Statement": [
    {
        "Effect": "Allow",
        "Action": [
        "athena:*"
        ],
        "Resource": [
        "*"
        ]
    },
    {
        "Effect": "Allow",
        "Action": [
        "s3:GetBucketLocation",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:ListBucketMultipartUploads",
        "s3:ListMultipartUploadParts",
        "s3:AbortMultipartUpload",
        "s3:CreateBucket",
        "s3:PutObject",
        "s3:PutBucketPublicAccessBlock"
        ],
        "Resource": [
        "arn:aws:s3:::aws-athena-query-results-*"
        ]
    },
    {
        "Effect": "Allow",
        "Action": [
        "s3:GetObject",
        "s3:ListBucket"
        ],
        "Resource": [
        "arn:aws:s3:::athena-examples*"
        ]
    },
    {
        "Effect": "Allow",
        "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation",
        "s3:ListAllMyBuckets"
        ],
        "Resource": [
        "*"
        ]
    }
    ]
}
